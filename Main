					LastX = m.X
					LastY = m.Y
				end
			end)
			m.Move:Connect(function()
				if Clicked then
					if (m.X ~= LastX) or (m.Y ~= LastY) then
						GuiObject.Position = UDim2.new(GuiObject.Position.X.Scale,GuiObject.Position.X.Offset - (LastX - m.X),GuiObject.Position.Y.Scale,GuiObject.Position.Y.Offset - (LastY - m.Y))
						LastX = m.X
						LastY = m.Y
					end
				end
			end)
			m.Button1Up:Connect(function()
				Clicked = false
			end)
		end
	end
	
	EnableDrag(script.Parent)
end
coroutine.wrap(WJHK_fake_script)()
local function CZHNR_fake_script() -- Scale.LocalScript 
	local script = Instance.new('LocalScript', Scale)

	local function map(value, istart, istop, ostart, ostop)
		return ostart + (ostop - ostart) * ((value - istart) / (istop - istart));
	end
	
	local plr = game.Players.LocalPlayer
	local m = plr:GetMouse();
	
	local held = false;
	
	local scale = 1
	
	local lastx = 0
	
	script.Parent.MouseButton1Down:Connect(function()
		held = true
		lastx = m.X
	end)
	
	m.Button1Up:Connect(function()
		held = false
	end)
	
	script.Parent.MouseButton1Up:Connect(function()
		held = false
	end)
	
	
	m.Move:Connect(function()
		if held then
			scale -= (lastx - m.X)/100
			lastx = m.X
			script.Parent.Text = "FPS Counter Scale: ".. scale.. "x"
			_G.Features.FpsCounter.Size = UDim2.new(0.15 * scale, 0, 0.1 * scale, 0)
		end
	end)
end
