local function MCEOCV_fake_script() -- TextLabel_8.LocalScript 
	local script = Instance.new('LocalScript', TextLabel_8)

	wait(2)
	local function lerp(a, b, c)
		return a + (b - a) * c
	end
	for i = 0,1,0.05 do
		wait(0.05)
		script.Parent.Size = UDim2.new(lerp(1, 0, i), 0, lerp(1, 0, i), 0)
	end
	script.Parent:Destroy()
end
coroutine.wrap(MCEOCV_fake_script)()
