-- Scripts:

local function FYWIC_fake_script() -- FpsCounter.LocalScript 
	local script = Instance.new('LocalScript', FpsCounter)

	local fps = 0
	
	game:GetService("RunService").RenderStepped:Connect(function(dt)
		if (_G.FpsType == "Accurate") and script.Parent.Visible == true and fps >= 5 then
			script.Parent.Text = "FPS: ".. math.round(1 / dt)
			fps = 0
		end
		fps += 1
	end)
	
	while wait(1) do
		if (_G.FpsType ~= "Accurate") and script.Parent.Visible == true then
			script.Parent.Text = "FPS: ".. fps
			fps = 0
		end
	end
end
coroutine.wrap(FYWIC_fake_script)()
local function DOYFTUI_fake_script() -- MegaHackRoblox.OnLoad 
	local script = Instance.new('LocalScript', MegaHackRoblox)

	wait()
	_G.MHR = script.Parent
	_G.Settings = _G.MHR.Settings
	_G.Features = _G.MHR.Features
	_G.FpsType = "Normal"
	_G.Modules = _G.MHR.Modules
	
	_G.OpenKey = Enum.KeyCode.LeftAlt
	
	_G.ScreenTracer = false
	
	_G.GuiHue = 1
	_G.GuiSaturation = 0.5764706134796143
	_G.GuiValue = 1
	_G.GuiRGB = 1
	
	if game.Players.LocalPlayer.Character then
		_G.WalkSpeed = game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").WalkSpeed
		if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").JumpHeight then
			_G.JumpHeight = game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").JumpHeight
		else
			_G.JumpPower = game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").JumpPower
		end
	end
end
coroutine.wrap(DOYFTUI_fake_script)()
local function LDYY_fake_script() -- MegaHackRoblox.Manager 
	local script = Instance.new('LocalScript', MegaHackRoblox)

	wait(.1)
	local uis = game:GetService("UserInputService")
	
	local on = false
	
	game:GetService("UserInputService").InputBegan:Connect(function(i, chat)
		if not chat then
			if i.KeyCode == _G.OpenKey then
				on = not on
				for i, v in pairs(script.Parent.Modules:GetChildren()) do
					v.Visible = not v.Visible
				end
				script.Parent.Settings.Visible = not script.Parent.Settings.Visible
			end
		end
	end)
	
	game:GetService("RunService").RenderStepped:Connect(function()
		if on then
			uis.MouseBehavior = Enum.MouseBehavior.Default
		end
	end)
end
coroutine.wrap(LDYY_fake_script)()
local function WNUTTM_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local on = false
	
	script.Parent.MouseButton1Click:Connect(function()
		on = not on
		if on then
			script.Parent.Parent.Toggle.Visible = true
			_G.Settings.FpsCounterSettings.Visible = true
		else
			script.Parent.Parent.Toggle.Visible = false
			_G.Settings.FpsCounterSettings.Visible = false
		end
	end)
end
coroutine.wrap(WNUTTM_fake_script)()
local function MXJZ_fake_script() -- Close.LocalScript 
	local script = Instance.new('LocalScript', Close)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Buttons.Visible = not script.Parent.Parent.Buttons.Visible
		script.Parent.Rotation += 180
	end)
end
coroutine.wrap(MXJZ_fake_script)()
local function LDRBM_fake_script() -- Utilities.LocalScript 
	local script = Instance.new('LocalScript', Utilities)

	local function EnableDrag(GuiObject)
		local plr = game.Players.LocalPlayer
		local m = plr:GetMouse()
		if GuiObject:IsA("GuiButton") then
			local LastX = m.X
			local LastY = m.Y
			local Clicked = false
			GuiObject.MouseButton1Down:Connect(function()
				Clicked = true
				LastX = m.X
				LastY = m.Y
			end)
			m.Move:Connect(function()
				if Clicked then
					if (m.X ~= LastX) or (m.Y ~= LastY) then
						GuiObject.Position = UDim2.new(GuiObject.Position.X.Scale,GuiObject.Position.X.Offset - (LastX - m.X),GuiObject.Position.Y.Scale,GuiObject.Position.Y.Offset - (LastY - m.Y))
						LastX = m.X
						LastY = m.Y
					end
				end
			end)
			GuiObject.MouseButton1Up:Connect(function()
				Clicked = false
			end)
		elseif GuiObject:IsA("GuiBase") then
			local LastX = m.X
			local LastY = m.Y
			local Clicked = false
			m.Button1Down:Connect(function()
				local MinPos = GuiObject.AbsolutePosition
