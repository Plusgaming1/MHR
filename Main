local function EWJI_fake_script() -- TextButton_3.LocalScript 
	local script = Instance.new('LocalScript', TextButton_3)

	local on = false
	
	script.Parent.MouseButton1Click:Connect(function()
		on = not on
		if on then
			script.Parent.Parent.Toggle.Visible = true
		else
			script.Parent.Parent.Toggle.Visible = false
		end
	end)
	
	local last = {}
	
	game:GetService("RunService").RenderStepped:Connect(function()
		for i, v in pairs(last) do
			v.CanCollide = true
		end
		last = {}
		if on then
			for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
				if v:IsA("BasePart") then
					local temp = v.Touched:Connect(function()
						
					end)
					local t = v:GetTouchingParts()
					temp:Disconnect()
					for _, a in pairs(t) do
						if not a:IsDescendantOf(game.Players.LocalPlayer.Character) then
							if a:IsA("BasePart") then
								local ray = Ray.new(game.Players.LocalPlayer.Character.HumanoidRootPart.Position, Vector3.new(0, -game.Players.LocalPlayer.Character.Humanoid.HipHeight*2, 0))
								local ignore = RaycastParams.new()
								ignore.FilterDescendantsInstances = {game.Players.LocalPlayer.Character}
								ignore.FilterType = Enum.RaycastFilterType.Blacklist
								local result = workspace:Raycast(ray.Origin, ray.Direction, ignore)
								if result then
									if result.Instance ~= a then
										if (a.CanCollide == true) and (a.Position.Y + a.Size.Y/2 > game.Players.LocalPlayer.Character.HumanoidRootPart.Position.Y) then
											table.insert(last, a)
											a.CanCollide = false
										end
									end
								else
									if (a.CanCollide == true) and (a.Position.Y + a.Size.Y/2 > game.Players.LocalPlayer.Character.HumanoidRootPart.Position.Y) then
										table.insert(last, a)
										a.CanCollide = false
									end
								end
							end
						end
					end
				end
			end
		end
	end)
end
