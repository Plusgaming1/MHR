local function BIKDUG_fake_script() -- TextButton_6.LocalScript 
	local script = Instance.new('LocalScript', TextButton_6)

	local on = false
	
	script.Parent.MouseButton1Click:Connect(function()
		on = not on
		if on then
			script.Parent.Parent.Toggle.Visible = true
		else
			script.Parent.Parent.Toggle.Visible = false
		end
	end)
	
	local tracers = {}
	
	local function trace(plr, position, color)
		local cam = game.Workspace.CurrentCamera
		local pos, visible = cam:WorldToScreenPoint(position)
		
		if visible then
			local e = Instance.new("TextLabel", _G.Features.Health)
			e.Position = UDim2.new(0, pos.X, 0, pos.Y)
			e.Text = "Name: ".. plr.Name.. ", Health: ".. plr.Character.Humanoid.Health.. ", Distance: ".. (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - position).Magnitude 
			e.AnchorPoint = Vector2.new(0.5, 0.5)
			e.TextScaled = true
			local pos2 = cam:WorldToScreenPoint(Vector3.new(position.X, position.Y + plr.Character.HumanoidRootPart.Size.Y/2, position.Z))
			local pos3 = cam:WorldToScreenPoint(Vector3.new(position.X, position.Y - plr.Character.HumanoidRootPart.Size.Y/2, position.Z))
			e.Size = UDim2.new(1, 0, 0, (Vector2.new(pos2.X, pos2.Y) - Vector2.new(pos3.X, pos3.Y)).Magnitude)
			e.BackgroundTransparency = 1
			e.TextColor3 = color
			table.insert(tracers, e)
		end
	end
	
	game:GetService("RunService").RenderStepped:Connect(function()
		for i, v in pairs(tracers) do
			v:Destroy()
		end
		if on then
			for i, v in pairs(game.Players:GetChildren()) do
				if v.Character and v.Character.HumanoidRootPart and v ~= game.Players.LocalPlayer then
					trace(v, v.Character.HumanoidRootPart.Position, v.TeamColor.Color)
				end
			end
		end
