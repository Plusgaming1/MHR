local function BHPHY_fake_script() -- Speed.LocalScript 
	local script = Instance.new('LocalScript', Speed)

	wait(0.5)
	local plr = game.Players.LocalPlayer
	local m = plr:GetMouse();
	
	local held = false;
	
	local scale = 16
	if _G.WalkSpeed then
		scale = _G.WalkSpeed
	end
	
	script.Parent.Text = "WalkSpeed: ".. scale
	
	local lastx = 0
	
	script.Parent.MouseButton1Down:Connect(function()
		held = true
		lastx = m.X
	end)
	
	m.Button1Up:Connect(function()
		held = false
	end)
	
	script.Parent.MouseButton1Up:Connect(function()
		held = false
	end)
	
	
	m.Move:Connect(function()
		if held then
			scale -= (lastx - m.X)/10
			lastx = m.X
			
			script.Parent.Text = "WalkSpeed: ".. scale
			
			game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").WalkSpeed = scale
		end
	end)
	
	game:GetService("RunService").RenderStepped:Connect(function()
		if scale ~= _G.WalkSpeed then
			game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").WalkSpeed = scale
		end
	end)
end
