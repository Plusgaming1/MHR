local function MYTLKR_fake_script() -- TextButton_4.LocalScript 
	local script = Instance.new('LocalScript', TextButton_4)

	local on = false
	
	script.Parent.MouseButton1Click:Connect(function()
		on = not on
		if on then
			script.Parent.Parent.Toggle.Visible = true
		else
			script.Parent.Parent.Toggle.Visible = false
		end
	end)
	
	local players = {}
	
	local esp = {}
	
	local function createesp(part, color)
		local e = Instance.new("BoxHandleAdornment", part)
		e.Adornee = part
		e.Size = part.Size
		e.Transparency = 0.5
		e.Visible = true
		e.Color3 = color
		e.AlwaysOnTop = true
		e.Visible = true
		e.ZIndex = 1
		table.insert(esp, e)
	end
	
	local function espe(char, color)
		for i, v in pairs(char:GetChildren()) do
			if v:IsA("BasePart") then
				createesp(v, color)
			end
		end
		table.insert(players, char)
	end
	
	game:GetService("RunService").RenderStepped:Connect(function()
		if on then
			for i, v in pairs(game.Players:GetChildren()) do
				if v ~= game.Players.LocalPlayer then
					local canuse = true
					for i, v_ in pairs(players) do
						if v_ == v.Character then
							canuse = false
						end
					end
					if canuse and v:IsA("Player") then
						espe(v.Character, v.TeamColor.Color)
						table.insert(players, v)
					end
				end
			end
		else
			for i, v in pairs(esp) do
				v:Destroy()
			end
			players = {}
		end
	end)
end
coroutine.wrap(MYTLKR_fake_script)()
local function YSCJH_fake_script() -- TextButton_5.LocalScript 
	local script = Instance.new('LocalScript', TextButton_5)

	local on = false
	
	local m = game.Players.LocalPlayer:GetMouse()
	
	script.Parent.MouseButton1Click:Connect(function()
		on = not on
		if on then
			script.Parent.Parent.Toggle.Visible = true
		else
			script.Parent.Parent.Toggle.Visible = false
		end
	end)
	
	local tracers = {}
	
	local function trace(position, color)
		local cam = game.Workspace.CurrentCamera
		local pos, visible = cam:WorldToScreenPoint(position)
		
		if visible == not _G.ScreenTracer then
			local e = Instance.new("Frame", _G.Features.Tracers)
			e.Position = UDim2.new(0, m.X, 0, m.Y)
			e.AnchorPoint = Vector2.new(0.5, 0.5)
			
			local rotation = math.deg(math.atan2(pos.Y - m.Y, pos.X - m.X))
			e.Rotation = rotation - 90
			local size = (e.AbsolutePosition - Vector2.new(pos.X, pos.Y)).Magnitude
			e.Size = UDim2.new(0, 0, 0, size)
			e.Position = e.Position:Lerp(UDim2.new(0, pos.X, 0, pos.Y), 0.5)
			e.BorderColor3 = color
			table.insert(tracers, e)
		end
	end
	
	game:GetService("RunService").RenderStepped:Connect(function()
		for i, v in pairs(tracers) do
			v:Destroy()
		end
		if on then
			for i, v in pairs(game.Players:GetChildren()) do
				if v.Character and v.Character.HumanoidRootPart then
					trace(v.Character.HumanoidRootPart.Position, v.TeamColor.Color)
				end
			end
		end
	end)
end
