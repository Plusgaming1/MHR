		lastx = m.X
	end)
	
	m.Button1Up:Connect(function()
		held = false
	end)
	
	script.Parent.MouseButton1Up:Connect(function()
		held = false
	end)
	
	
	m.Move:Connect(function()
		if held then
			scale -= (lastx - m.X)/10
			lastx = m.X
			
			if Type == 0 then
				script.Parent.Text = "Jump Power: ".. scale
			else
				script.Parent.Text = "Jump Height: ".. scale
			end
			
			if Type == 1 then
				game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").JumpHeight = scale
			else
				game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").JumpPower = scale
			end
		end
	end)
	
	game:GetService("RunService").RenderStepped:Connect(function()
		if Type == 1 then
			if scale ~= _G.JumpHeight then
				game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").JumpHeight = scale
			end
		else
			if scale ~= _G.JumpPower then
				game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").JumpPower = scale
			end
		end
		
	end)
